{
  "expo": {
    "name": "Kaptur",
    "slug": "kaptur-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "kapturapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tonnom.kaptur",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Kaptur a besoin de votre micro pour enregistrer vos notes vocales à analyser."
      }
    },
    "android": {
      "package": "com.tonnom.kaptur",
      "permissions": ["AUDIO_RECORDING"]
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-av",
        {
          "microphonePermission": "Kaptur a besoin de votre micro pour enregistrer vos notes vocales à analyser."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "da92aaff-40ce-4586-9ded-87b0be77e9a6"
      }
    },
    "owner": "maximekirch"
  }
}
