{
  "expo": {
    "name": "Relevo",
    "slug": "kaptur-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/relevo_icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#000000",
    "splash": {
      "image": "./assets/images/relevo_splash_screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.maximekirch.relevo",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Relevo a besoin de votre micro pour enregistrer vos notes vocales à analyser.",
        "ITSAppUsesNonExemptEncryption": false,
        "UIUserInterfaceStyle": "Dark"
      }
    },
    "android": {
      "package": "com.maximekirch.relevo",
      "permissions": [
        "AUDIO_RECORDING",
        "android.permission.RECORD_AUDIO",
        "android.permission.MODIFY_AUDIO_SETTINGS"
      ]
    },
    "plugins": [
      "expo-router",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "relevo-app",
          "organization": "maxime-kirch-ri"
        }
      ],
      [
        "expo-audio",
        {
          "microphonePermission": "Relevo a besoin de votre micro pour enregistrer vos notes vocales à analyser."
        }
      ],
      "expo-asset",
      "@sentry/react-native",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "da92aaff-40ce-4586-9ded-87b0be77e9a6"
      }
    },
    "owner": "maximekirch"
  }
}
