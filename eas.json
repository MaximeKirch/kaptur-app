{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "SENTRY_DIST": "dev"
      }
    },
    "preview": {
      "distribution": "internal",
      "developmentClient": false,
      "ios": {
        "simulator": false
      },
      "env": {
        "SENTRY_DIST": "preview"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "SENTRY_DIST": "production"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
